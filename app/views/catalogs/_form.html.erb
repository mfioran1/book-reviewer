<%= form_for list do |f| %>
    <%= f.label :name %>
    <%= f.text_field :name %>
    <br>
    <br>
    <%= f.label :description %>
    <%= f.text_area :description %>
    <br>
    <br>
    <%= f.collection_check_boxes :book_ids, catalog.books, :id, :title %>
    <br>
    <br>
    <%= f.submit %>
<% end %>
    


