<%= form_for catalog do |f| %>
    <%= f.label :name %>
    <%= f.text_field :name %>
    <br>
    <br>
    <%= f.label :description %>
    <%= f.text_area :description %>
    <br>
    <br>
    <%= f.collection_check_boxes :book_ids, Book.all, :id, :title do |box|%>
        <%= box.check_box %><%= box.label {box.text} %>
    <% end %>
    <br>
    <br>
    <%= f.submit %>
<% end %>
    


