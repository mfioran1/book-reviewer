<h1>Log in Here!</h1>

<% if flash[:message] %>
<%= flash[:message] %>
<% end %>

<%= form_for :user do |f| %>
    
    <%= f.label :email %>
    <%= f.email_field :email %>
    <br>
    <br>
    <%= f.label :password %>
    <%= f.password_field :password %>
    <br>
    <br>
    <%= f.submit "Sign Up" %>
<% end %>